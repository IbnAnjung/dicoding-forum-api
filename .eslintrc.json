{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-undef":"off",
        "no-underscore-dangle": "off",
        "class-methods-use-this":"off",
        "no-unused-vars":"off",
        "linebreak-style":"off"
    }
}
